{
    "version": "0.1.0",
    "configurations": [
        {
            "type": "node",
            "request": "attach",
            "name": "Attach to Node process",
            "port": 9229,
            "restart": true,
            "sourceMaps": true,
            "smartStep": true,
            "stopOnEntry": false,
            "timeout": 50000,
            "localRoot": "${workspaceFolder}",
            "remoteRoot": "${workspaceFolder}",
            "skipFiles": ["${workspaceRoot}/node_modules/**/*.js", "<node_internals>/**/*.js", "<node_internals>/**"],
            "outFiles": ["${workspaceFolder}/lib/**/*.js"]
        }
    ]
}
