  "description": "Backtrace Vite plugin",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "engines": {
      "node": ">=14"
  },
  "devDependencies": {},
  "scripts": {
      "build": "tsc -b ./tsconfig.build.json",
      "clean": "tsc -b ./tsconfig.build.json --clean && rimraf \"lib\"",
