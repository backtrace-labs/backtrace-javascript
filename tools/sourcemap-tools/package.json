{
    "name": "@backtrace/sourcemap-tools",
    "version": "0.2.3",
    "description": "Backtrace-JavaScript sourcemap tools",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "engines": {
        "node": ">=14"
    },
    "scripts": {
        "build": "tsc",
        "clean": "tsc -b --clean && rimraf \"lib\"",
