        "url": "https://github.com/backtrace-labs/backtrace-javascript/issues"
    },
    "homepage": "https://github.com/backtrace-labs/backtrace-javascript#readme",
    "devDependencies": {
        "@types/decompress": "^4.2.4",
        "@types/jest": "^29.5.1",
        "@types/tar-stream": "^2.2.2",
        "decompress": "^4.2.1",
        "jest": "^29.5.0",
        "nock": "^13.3.1",
        "source-map": "^0.7.4",
        "ts-jest": "^29.1.0",
        "typescript": "^5.0.4"
    },
